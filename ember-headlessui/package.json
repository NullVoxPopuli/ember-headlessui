{
  "name": "ember-headlessui",
  "version": "0.12.3",
  "description": "The default blueprint for ember-cli addons.",
  "repository": {
    "type": "git",
    "url": "https://github.com/GavinJoyce/ember-headlessui.git"
  },
  "license": "MIT",
  "author": "",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "addon-main.cjs",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "start": "concurrently 'npm:watch:*'",
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup -c ./rollup.config.js",
    "build:docs": "cp ../README.md ./README.md",
    "watch:js": "rollup -c --watch --no-watch.clearScreen",
    "lint": "concurrently 'npm:lint:js'",
    "lint:fix": "concurrently 'npm:lint:js:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "test": "echo 'Addon does not have tests, run tests in test-app'",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run build"
  },
  "dependencies": {
    "@embroider/addon-shim": "1.6.0",
    "@ember/render-modifiers": "^2.0.0",
    "ember-concurrency": "^2.1.2",
    "ember-element-helper": "^0.6.0",
    "ember-modifier": "^3.0.0",
    "ember-set-helper": "^2.0.1",
    "ember-truth-helpers": "^3.0.0",
    "focus-trap": "^6.7.1",
    "tracked-maps-and-sets": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-syntax-decorators": "7.17.0",
    "@babel/plugin-proposal-decorators": "7.17.9",
    "@embroider/addon-dev": "1.6.0",
    "@nullvoxpopuli/eslint-configs": "2.2.16",
    "concurrently": "7.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-decorator-position": "4.0.1",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "rollup": "2.72.0",
    "babel-eslint": "10.1.0",
    "eslint": "^7.0.0",
    "@babel/plugin-transform-typescript": "7.16.8",
    "@babel/preset-typescript": "7.16.7",
    "rollup-plugin-ts": "2.0.7",
    "typescript": "4.6.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "./addon-main.cjs",
    "app-js": {}
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "volta": {
    "extends": "../package.json"
  },
  "types": "dist"
}